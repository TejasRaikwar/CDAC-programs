package day3;
public class Employee {
    private static long nextEmpNo = 123465; // Tracks the next available ID (static)
    private final long empNo; // Unique per employee (final, no setter)
    private String name;
    private double salary;

    public Employee() {
        this.empNo = nextEmpNo++; // Assign & auto-increment
        this.name = "Unknown";
        this.salary = 0.0;
    }

    public Employee(String name, double salary) {
        this.empNo = nextEmpNo++; // Assign & auto-increment
        this.name = name;
        this.salary = salary;
    }

    // Getters (no setter for empNo)
    public long getEmpNo() {
        return empNo;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public double getSalary() {
        return salary;
    }

    public void setSalary(double salary) {
        this.salary = salary;
    }

    public String printData() {
        return "Employee [EmpNo=" + empNo + ", Name=" + name + ", Salary=" + salary + "]";
    }
}